{
  "name": "video-stream-processor",
  "version": "1.0.0",
  "description": "Video stream file processing application",
  "main": "src/index.ts",
  "scripts": {
    "start": "docker-compose up",
    "start:detached": "docker-compose up -d",
    "stop": "docker-compose down",
    "start:node": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:generate_video": "ts-node-dev --respawn --transpile-only src/generate_video.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start:redis:minio": "docker-compose up -d redis redis-insights minio"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.5",
    "express": "5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "minio": "8.0.5",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "puppeteer": "^22.0.0",
    "puppeteer-screen-recorder": "^3.0.6",
    "redis": "5.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "5.0.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "22.15.21",
    "@types/puppeteer": "^7.0.4",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.3"
  },
  "overrides": {
    "puppeteer-screen-recorder": {
      "puppeteer": "^22.0.0"
    }
  }
}
